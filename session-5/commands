# aws ecr get-login-password --region us-east-2 | docker login --username AWS --password-stdin 160071257600.dkr.ecr.us-east-2.amazonaws.com

# docker push 160071257600.dkr.ecr.us-east-2.amazonaws.com/david-vasconcelos-iac-lab-crud-app:latest